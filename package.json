{
  "name": "reactjs_test_assignment_povio",
  "version": "1.2.2",
  "description": "ReactJS Test Assignment - Povio",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "clean": "rimraf dist",
    "test": "cross-env NODE_ENV=production jest",
    "test-w": "nodemon --exec \"npm test\"",
    "build": "npm run clean && cross-env NODE_ENV=production webpack && cross-env NODE_ENV=production node server.js",
    "builddev": "npm run clean && cross-env NODE_ENV=development webpack && cross-env NODE_ENV=development node server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot"
  },
  "eslintIgnore": [
    "webpack.config.js",
    "public/assets",
    "public/__tests__",
    "test"
  ],
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "Rok Zigon",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.0",
    "postcss": "^6.0.23",
    "postcss-loader": "^2.1.6",
    "postcss-scss": "^1.0.6",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-enzyme-context": "^1.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^2.11.2",
    "webpack-hot-middleware": "^2.22.2",
    "why-did-you-update": "^0.1.1"
  },
  "dependencies": {
    "autoprefixer": "^8.6.5",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "helmet": "^3.12.1",
    "helmet-csp": "^2.7.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.13.2"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/public//setupTests.js",
    "testMatch": [
      "<rootDir>/public/__tests__/*.{js,jsx,mjs}",
      "<rootDir>/public/reducers/__tests__/*.{js,jsx,mjs}",
      "<rootDir>/test/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/public/__tests__/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}

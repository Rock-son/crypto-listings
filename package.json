  "version": "1.0.0",
  "description": "ReactJS Test Assignment - Povio",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "clean": "rimraf dist",
    "test-w": "nodemon --exec \"npm test\"",

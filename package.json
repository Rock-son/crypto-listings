    "builddev": "npm run clean && webpack --config webpack.config.dev.js && cross-env NODE_ENV=development node server.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js --hot"
  },
  "eslintIgnore": [
    "webpack.config.js",
    "webpack.config.dev.js",
    "public/assets"
  ],
